		<script>
			var workspace;
			var editor;
			var tm;
			$(function () {
				workspace = $.connection.workspace;
				//$.connection.editor.qs = "documentId=979";
				// $.connection.hub.qs = { "documentId": 979 };
				editor = $.connection.editor;
				tm = $.connection.translationmemory;

				$.connection.hub.start({ transport: 'longPolling' });

				workspace.client.ProjectNotify = ProjectNotify;
				workspace.client.DocumentNotify = DocumentNotify;
				editor.client.SegmentNotify = SegmentNotify;
				tm.client.TMNotify = TMNotify;
			});

			function ProjectNotify(data) {
				var projectChangeType = data.ProjectChangeType;
				var changeTypeString;
				switch (projectChangeType) {
					case 1:
						changeTypeString = "created";
						break;
					case 2:
						changeTypeString = "deleted";
						break;
					case 4:
						changeTypeString = "updated";
						break;
					case 8:
						changeTypeString = "UpdateTm";
						break;
					case 16:
						changeTypeString = "UpdateMT";
						break;
					case 32:
						changeTypeString = "UpdateGlossaries";
						break;
					default:
						changeTypeString = "unknown";
				}
				console.log("ProjectId=" + data.ProjectId + " Type:" + changeTypeString);
			};

			function DocumentNotify(data) {
				var documentChangeType = data.DocumentChangeType;
				var changeTypeString;
				switch (documentChangeType) {
					case 1:
						changeTypeString = "created";
						break;
					case 2:
						changeTypeString = "deleted";
						break;
					case 4:
						changeTypeString = "updated";
						break;
					case 8:
						changeTypeString = "UpdateTm";
						break;
					case 16:
						changeTypeString = "UpdateMT";
						break;
					case 32:
						changeTypeString = "UpdateGlossaries";
						break;
					case 64:
						changeTypeString = "SegmentsResolved";
						break;
					default:
						changeTypeString = "unknown";
				}
				alert("DocumentId=" + data.DocumentId + " Type:" + changeTypeString);
			};

			function SegmentNotify(data) {
				var segmentChangeType = data.SegmentChangeType;
				var changeTypeString;
				switch (segmentChangeType) {
					case 1:
						changeTypeString = "Updated";
						break;
					case 2:
						changeTypeString = "Confirmed";
						break;
					case 4:
						changeTypeString = "Locked";
						break;
					default:
						changeTypeString = "unknown";
				}
				alert("DocumentId=" + data.DocumentId + " SegmentId=" + data.SegmentId + " Type:" + changeTypeString);
			};
			
			function TMNotify(data) {
				var tmChangeType = data.TranslationMemoryChangeType;
				var changeTypeString;
				switch (tmChangeType) {
					case 1:
						changeTypeString = "Created";
						break;
					case 2:
						changeTypeString = "Deleted";
						break;
					default:
						changeTypeString = "unknown";
				}
				alert("TranslationMemoryId=" + data.TranslationMemoryId + " Type:" + changeTypeString);
			};
		</script>-->

